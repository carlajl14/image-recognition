{
    "version": 2,
    "builds": [
      {
        "src": "install.sh",
        "use": "@vercel/static-build"
      },
      {
        "src": "app.py",
        "use": "@vercel/python",
        "config": {
          "runtime": "python3.9"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ]
  }
  